<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

    <util:list id="converters">

        <bean class="converters.StringArrayToCollectionEtiquetas"></bean>

        <bean class="converters.ActorToStringConverter"></bean>
        <bean class="converters.StringToActorConverter"></bean>

        <bean class="converters.AdminToStringConverter"></bean>
        <bean class="converters.StringToAdminConverter"></bean>

        <bean class="converters.EstudioToStringConverter"></bean>
        <bean class="converters.StringToEstudioConverter"></bean>

        <bean class="converters.EtiquetaToStringConverter"></bean>
        <bean class="converters.StringToEtiquetaConverter"></bean>

        <bean class="converters.ItemToStringConverter"></bean>
        <bean class="converters.StringToItemConverter"></bean>

        <bean class="converters.OfertaToStringConverter"></bean>
        <bean class="converters.StringToOfertaConverter"></bean>

        <bean class="converters.PagoToStringConverter"></bean>
        <bean class="converters.StringToPagoConverter"></bean>

        <bean class="converters.PeticionToStringConverter"></bean>
        <bean class="converters.StringToPeticionConverter"></bean>

        <bean class="converters.ProfesionalToStringConverter"></bean>
        <bean class="converters.StringToProfesionalConverter"></bean>

        <bean class="converters.RolToStringConverter"></bean>
        <bean class="converters.StringToRolConverter"></bean>

        <bean class="converters.UsuarioToStringConverter"></bean>
        <bean class="converters.StringToUsuarioConverter"></bean>

        <bean class="converters.TrabajoToStringConverter"></bean>
        <bean class="converters.StringToTrabajoConverter"></bean>

        <bean class="converters.ValoracionToStringConverter"></bean>
        <bean class="converters.StringToValoracionConverter"></bean>


    </util:list>

</beans>